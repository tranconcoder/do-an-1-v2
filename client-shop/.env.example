PORT=3001
REACT_APP_API_URL=https://localhost:3002

# OpenRouteService API Key (for Leaflet maps)
# Get your free API key from: https://openrouteservice.org/
REACT_APP_OPENROUTE_API_KEY=your_openroute_api_key_here

# Google Maps API Key (for development purposes)
# Get your API key from: https://console.cloud.google.com/
REACT_APP_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE
REACT_APP_GOOGLE_MAP_ID=YOUR_MAP_ID_FOR_ADVANCED_MARKERS

# Note: Copy this file to .env and adjust the values according to your environment